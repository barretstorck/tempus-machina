{
    "name": "barretstorck/tempus-machina",
    "type": "library",
    "description": "A PSR-20 compliant PHP clock library",
    "keywords": [
        "clock",
        "time",
        "psr20",
        "psr-20"
    ],
    "require": {
        "psr/clock": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^10",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "BarretStorck\\TempusMachina\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BarretStorck\\TempusMachina\\Tests\\": "tests/"
        }
    }
}